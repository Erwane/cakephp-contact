{
    "name": "erwane/cakephp-contact",
    "description": "Cakephp plugin for manipulating Contact information (address, phone, ...)",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "contact", "phone", "address"],
    "license": "MIT",
    "authors": [
        {
            "name": "Erwane Breton",
            "email": "erwane@phea.fr"
        }
    ],

    "require": {
        "cakephp/cakephp": ">=3.2.0",
        "giggsey/libphonenumber-for-php": "7.*"
    },

    "require-dev": {
        "phpunit/phpunit": "5.3.*",
        "cakephp/cakephp-codesniffer": "dev-master"
    },

    "autoload": {
        "psr-4": {
            "Contact\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Contact\\Test\\": "tests"
        }
    },

    "minimum-stability": "beta"
}
