{
    "name": "erwane/cakephp-contact",
    "description": "Cakephp plugin for manipulating Contact information (address, phone, ...)",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "contact", "phone", "address"],
    "license": "MIT",
    "authors": [
        {
            "name": "Erwane Breton",
            "email": "erwane@phea.fr"
        }
    ],

    "require": {
        "cakephp/cakephp": "^4",
        "giggsey/libphonenumber-for-php": "^8.0"
    },

    "require-dev": {
        "cakephp/cakephp-codesniffer": "^4.0",
        "php-parallel-lint/php-parallel-lint": "^1.1",
        "phpro/grumphp": "^0.18",
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^5.0"
    },

    "autoload": {
        "psr-4": {
            "Contact\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Contact\\Test\\": "tests"
        }
    },

    "scripts": {
        "cscheck": "phpcs -p src/ tests/",
        "csfix": "phpcbf -p src/ tests/"
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
